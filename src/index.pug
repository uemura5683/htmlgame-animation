                img(src="/images/pug.svg" alt="Pug" width="80" class="c-next-images c-next-images3")
              span.c-next-anime.c-next-anime4
                img(src="/images/ts.svg" alt="TypeScript" width="120" class="c-next-images c-next-images4")
              span.c-next-anime.c-next-anime5
                img(src="/images/ts.svg" alt="TypeScript" width="120" class="c-next-images c-next-images5")
