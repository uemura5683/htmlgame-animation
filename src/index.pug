            .c-title Rings
            .c-rings-wrap-inner
              span.c-rings-images
                img(src="/images/go.svg" alt="Go" width="40" class="c-rings-images-inner")
              span.c-rings-images
                img(src="/images/py.svg" alt="Phython" width="40" class="c-rings-images-inner")
              span.c-rings-images
                img(src="/images/pug.svg" alt="Pug" width="40" class="c-rings-images-inner")
